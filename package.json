{
  "name": "actions-telegram-notification",
  "version": "0.0.0",
  "private": true,
  "description": "telegram notification",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "lint": "eslint src/**/*.ts",
    "b:main": "ncc build src/main.ts -o dist/main/",
    "bundle": "yarn b:main",
    "local": "NODE_ENV=development node --inspect=5858 --require ts-node/register src/local.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "telegram",
    "notification"
  ],
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "axios": "^0.19.2",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@octokit/webhooks": "^7.6.5",
    "@types/jest": "^26.0.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.24",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "@zeit/ncc": "^0.22.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "jest": "^26.1.0",
    "jest-circus": "^26.1.0",
    "js-yaml": "^3.13.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.6.4"
  }
}
